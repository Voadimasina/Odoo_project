        <field name="name">Analyse Prestation</field>
        <field name="code">analyse.prestation</field>
        <field name="prefix">PR</field>
        <field name="padding">5</field>
    </record>

    <record id="seq_prelevement" model="ir.sequence">
        <field name="name">Radiation Prélèvement</field>
        <field name="code">radiation.prelevement</field>
